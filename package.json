{
  "name": "lakhemern",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./build/index.js",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-babel",
    "server:build": "npm install && npm run build",
    "server": "nodemon src/index.js --exec babel-node --presets babel-preset-env",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run server:build"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.2.1",
    "concurrently": "^5.2.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.5.9",
    "mongoose": "^5.9.25",
    "morgan": "~1.9.1",
    "nodemailer": "^6.4.10",
    "pug": "2.0.0-beta11",
    "babel-preset-env": "^1.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "mocha": "^8.0.1",
    "mockgoose": "^8.0.4",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2"
  }
}
